{
  "name": "findstate-cli",
  "version": "1.0.0",
  "description": "A CLI for querying data optimized for Real Estate",
  "main": "index.js",
  "bin": {
    "findstate-cli": "./dist/cli/index.js"
  },
  "type": "commonjs",
  "scripts": {
    "start": "node dist/cli/index.js",
    "dev": "nodemon src/cli/index.ts",
